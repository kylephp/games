<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="10001" id="com.kylelabs.watermaze3d" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Water Maze 3D</name>
    <description>
        Designed with love by Kyle Hoang
    </description>
    <author email="kyle.labs89@gmail.com" href="http://kylephp.github.io/">
       Kyle Labs
    </author>
    <icon src="res/logo.png" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~7.0.0" />
    <plugin name="cordova-plugin-admob-free" spec="^0.18.0" />
    <engine name="android" spec="^7.0.0" />
    <engine name="ios" spec="~4.5.5" />
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
